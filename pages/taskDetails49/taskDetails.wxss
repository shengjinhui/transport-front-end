page {
  width: 100%;
  height: 100%;
}

.information {
  font-size: 17px;
}

.number {
  font-size: 15px;
  color: #0ada9c;
  padding: 10px 0;
  border-bottom: 1px solid #efefef;
  text-align: center;
}

.line {
  margin-top: 9%;
}

.lineItem {
  display: inline-block;
  width: 43%;
  margin-left: 7%;
}

.address {
  margin-top: 9%;
  margin-left: 20%;
}

.taskStart {
  width: 70%;
  margin-top: 13%;
  text-align: center;
  background-color: #17c087;
  border-radius: 8px;
}
.none{
  height: 100%;
  width: 100%;
  text-align: center;
  background: #aaa;
  -webkit-text-stroke:0.2px #555;color: transparent;
}